{
  "name": "normalize-url-plus",
  "version": "1.5.4",
  "description": "normalize-url plus additional features to supercharge link normalization!",
  "repository": "JaneJeon/normalize-url-plus",
  "author": "Jane Jeon <git@janejeon.com>",
  "license": "LGPL-3.0",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "bin/",
    "dist/",
    "package.json",
    "LICENSE"
  ],
  "bin": "bin/normalize.js",
  "scripts": {
    "test": "jest",
    "update-data": "curl -L -s https://rules2.clearurls.xyz/data.minify.json > data/data.minify.json",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "lint:eslint": "eslint . --ignore-path .gitignore --cache --cache-location .cache/eslint",
    "build": "run-s build:*",
    "build:ts": "tsc",
    "clean": "run-s clean:*",
    "clean:jest": "rimraf coverage reports",
    "clean:ts": "rimraf dist **/*.tsbuildinfo",
    "clean:cache": "rimraf .cache",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "debug": "^4.3.2",
    "got-scraping": "^3.2.3",
    "got-ssrf": "^1.3.0",
    "ipaddr.js": "^2.0.1",
    "leven": "^4.0.0",
    "lodash": "^4.17.21",
    "mem": "^9.0.1",
    "normalize-url": "^7.0.2",
    "p-timeout": "^5.0.0",
    "quick-lru": "^6.0.1",
    "re2": "^1.16.0",
    "tld-extract": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@janejeon/eslint-config": "^2.2.0",
    "@janejeon/prettier-config": "^1.1.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "cacheable-lookup": "^6.0.4",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.2.1",
    "jest-junit": "^13.0.0",
    "lint-staged": "12.3.7",
    "nock": "^13.1.3",
    "node-notifier": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "skip-ci": "^1.0.4",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "normalize",
    "url",
    "uri",
    "address",
    "string",
    "normalization",
    "normalisation",
    "query",
    "querystring",
    "simplify",
    "strip",
    "trim",
    "canonical"
  ]
